"><value name="FROM"><shadow type="math_number"><field name="number">0</field></shadow><block type="variables_get" id="36pjDotN3?@+^a,X(VOV"><field name="VAR" id="WOk_)Mj*-KLa^@h^IcE^">currentVal</field></block></value></block></value><next><block type="display_number" id="Y+PXaeLakVUu*?$lpyqD"><value name="value"><shadow type="math_float_number"><field name="number">10</field></shadow><block type="variables_get" id="8jc9w^)k-mem~NMN3,PZ"><field name="VAR" id="WOk_)Mj*-KLa^@h^IcE^">currentVal</field></block></value><next><block type="action_wait_ms" id=";3CCU]g}-*H#Fzv:Tl?u"><value name="NUMBER"><shadow type="math_number" id=":c_GYLvU6^%!dtDD.}8f"><field name="number">1000</field></shadow></value></block></next></block></next></block></next></block></next></block></next></block></statement></block></next></block></next></block></next></block></next></block></next></block></statement></block><gogo xmlns="http://www.w3.org/1999/xhtml"><program_mode>block</program_mode><logo_code>to main
set buttonInput 0
set category 0
set i 0
set currentVal 0
set buttonChoice 0
  sendgmessage "data-open" "data-open"
  wait 500
  sendgmessage "ready-to-train" "ready"
  set i (0) 
  while ( i &lt; 10 )
  [
    show "Record data "
    waituntil [ sensor2 &gt; 500 ]
    set currentVal (sensor1) 
    sendgmessage "data-to-train" (totext currentVal)
    show currentVal
    wait 1000
    show "SELECT CATEGORY... "
    wait 1000
    show "RED = DARK, GREEN = LIGHT "
    waituntil [ ( sensor3 &gt; 50 )  ]
    set buttonChoice selectCategory (sensor3)   
    ifelse ( buttonChoice = 0 )
    [
      show "YOU CHOSE DARK "
    ] [
      if ( buttonChoice = 1 )
      [
        show "YOU CHOSE LIGHT "
      ]
    ]
    sendgmessage "data-to-train-category" (totext buttonChoice)
    set i i + 1
   ]
  forever
  [
    show "Record data to predict "
    waituntil [ sensor2 &gt; 500 ]
    set currentVal (sensor1) 
    sendgmessage "data-to-predict" (totext currentVal)
    show currentVal
    wait 1000
   ]
end

to selectCategory :buttonInput
  if (buttonInput &gt; 250) and (buttonInput &lt; 399)
  [
    set category (0) 
  ]
  if (buttonInput &gt; 400) and (buttonInput &lt; 600)
  [
    set category (1) 
  ]

   output category;

 end</logo_code><file_name>GOGOML</file_name><timestamp>1650565518172</timestamp><datetime_iso8601>2022-04-21T18:25:18.172Z</datetime_iso8601><locale>en</locale><is_local_file>true</is_local_file></gogo></xml>